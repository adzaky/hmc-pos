generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    directUrl = env("DIRECT_URL")
    url      = env("DATABASE_URL")
}

model Category {
    id String @id @default(uuid())

    name String
    productCount Int @default(0)

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}